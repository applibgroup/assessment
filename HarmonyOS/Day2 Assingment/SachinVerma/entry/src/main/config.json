{
  "app": {
    "bundleName": "com.example.assignment",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "apiVersion": {
      "compatible": 5,
      "target": 5,
      "releaseType": "Release"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.example.assignment",
    "name": ".MyApplication",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "com.example.assignment.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      },
      {
        "permissions": [
          "com.example.assignment.DataAbilityShellProvider.PROVIDER"
        ],
        "name": "com.example.assignment.DataAbility",
        "icon": "$media:icon",
        "type": "data",
        "visible": true,
        "uri": "dataability://com.example.assignment.DataAbility"
      }
    ]
  }
}